{
  "name": "electron-example",
  "version": "0.0.1",
  "description": "electron example",
  "main": "./dist/main.js",
  "scripts": {
    "start:react": "concurrently \"pnpm -F example-main start\" \"pnpm -F example-views start\"",
    "build:react": "pnpm -F usine-* build && pnpm -F * build",
    "package": "zx ./scripts/package.mjs",
    "pure-package": "rimraf out && electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinjobs/electron-example.git"
  },
  "keywords": [
    "template",
    "react",
    "electron",
    "typescript"
  ],
  "author": "Kevin Jobs <me@kevinjobs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinjobs/electron-example/issues"
  },
  "homepage": "https://github.com/kevinjobs/electron-example#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-base": "^6.1.1",
    "concurrently": "latest",
    "electron": "24.0.0",
    "shelljs": "^0.8.5",
    "zx": "^4.3.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
